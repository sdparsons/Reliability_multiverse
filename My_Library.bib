@article{carl_positive_2013-1,
	title = {Positive emotion regulation in emotional disorders: {A} theoretical review},
	volume = {33},
	issn = {02727358},
	shorttitle = {Positive emotion regulation in emotional disorders},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0272735813000068},
	doi = {10.1016/j.cpr.2013.01.003},
	abstract = {Conceptualizations of emotion regulation have led to the identiﬁcation of cognitive and behavioral regulatory abnormalities that contribute to the development and maintenance of emotional disorders. However, existing research on emotion regulation in anxiety and mood disorders has primarily focused on the regulation of negative emotions rather than positive emotions. Recent ﬁndings indicate that disturbances in positive emotion regulation occur across emotional disorders, and may be a generative target for treatment research. The aims of this paper are to: 1. Present a transdiagnostic model of positive emotion disturbances in emotional disorders; 2. Review evidence for disturbances in positive emotion regulation in emotional disorders across categories of emotion regulation; and 3. Propose treatment strategies that may address these disturbances.},
