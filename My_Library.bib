	pages = {1--49},
	file = {Kalisch, Müller, Tüscher - 2014 - A conceptual framework for the neurobiological study of resilience.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Kalisch, Müller, Tüscher - 2014 - A conceptual framework for the neurobiological study of resilience.pdf:application/pdf}
