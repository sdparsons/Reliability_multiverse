	pages = {2689--97},
	file = {Browning et al. - 2011 - Using an experimental medicine model to explore combination effects of pharmacological and cognitive interventi.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Browning et al. - 2011 - Using an experimental medicine model to explore combination effects of pharmacological and cognitive interventi.pdf:application/pdf}
