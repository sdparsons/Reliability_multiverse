	file = {Garland et al. - 2010 - Upward spirals of positive emotions counter downwa.pdf:C\:\\Users\\Sam\\Zotero\\storage\\MND4B2SU\\Garland et al. - 2010 - Upward spirals of positive emotions counter downwa.pdf:application/pdf}
}

