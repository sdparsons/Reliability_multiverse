	pages = {285--312},
	file = {Gotlib and Joormann - 2010 - Cognition and Depression Current Status and Futur.pdf:C\:\\Users\\Sam\\Zotero\\storage\\L5GY8VLL\\Gotlib and Joormann - 2010 - Cognition and Depression Current Status and Futur.pdf:application/pdf}
}

