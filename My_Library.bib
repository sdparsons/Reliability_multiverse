	file = {Notebaert et al. - 2015 - Validation of a novel attentional bias modificatio.pdf:C\:\\Users\\Sam\\Zotero\\storage\\QUZMGMJF\\Notebaert et al. - 2015 - Validation of a novel attentional bias modificatio.pdf:application/pdf}
}

@article{chen_attentional_2015,
