	file = {Cox et al. - 2016 - Cognitive bias modification as a strategy to reduc.pdf:C\:\\Users\\Sam\\Zotero\\storage\\6D5ILYIU\\Cox et al. - 2016 - Cognitive bias modification as a strategy to reduc.pdf:application/pdf}
