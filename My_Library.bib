	publisher = {Guilford},
	author = {Gawronski, B and Deutsch, R and Banse, R},
	editor = {Klauer, K and Stahl, C and Voss, A},
	year = {2011},
	note = {Issue: 1},
	keywords = {reliability},
	pages = {78--123},
	file = {Gawronski, Deutsch, Banse - 2011 - Response interference tasks as indirect measures of automatic associations.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Gawronski, Deutsch, Banse - 2011 - Response interference tasks as indirect measures of automatic associations.pdf:application/pdf}
