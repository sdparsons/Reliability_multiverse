@article{den_ouden_dissociable_2013,
	title = {Dissociable {Effects} of {Dopamine} and {Serotonin} on {Reversal} {Learning}},
	volume = {80},
	issn = {08966273},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0896627313007897},
	doi = {10.1016/j.neuron.2013.08.030},
	abstract = {Serotonin and dopamine are speculated to subserve motivationally opponent functions, but this hypothesis has not been directly tested. We studied the role of these neurotransmitters in probabilistic reversal learning in nearly 700 individuals as a function of two polymorphisms in the genes encoding the serotonin and dopamine transporters (SERT: 5HTTLPR plus rs25531; DAT1 30UTR VNTR). A double dissociation was observed. The SERT polymorphism altered behavioral adaptation after losses, with increased lose-shift associated with L0 homozygosity, while leaving unaffected perseveration after reversal. In contrast, the DAT1 genotype affected the inﬂuence of prior choices on perseveration, while leaving lose-shifting unaltered. A model of reinforcement learning captured the dose-dependent effect of DAT1 genotype, such that an increasing number of 9R-alleles resulted in a stronger reliance on previous experience and therefore reluctance to update learned associations. These data provide direct evidence for doubly dissociable effects of serotonin and dopamine systems.},
	language = {en},
	number = {4},
	urldate = {2019-01-16},
	journal = {Neuron},
	author = {den Ouden, Hanneke E.M. and Daw, Nathaniel D. and Fernandez, Guillén and Elshout, Joris A. and Rijpkema, Mark and Hoogman, Martine and Franke, Barbara and Cools, Roshan},
