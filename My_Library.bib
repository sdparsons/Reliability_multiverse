@article{Notebaert2014,
	title = {When we should worry more: using cognitive bias modification to drive adaptive health behaviour.},
	volume = {9},
	issn = {1932-6203},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3885669&tool=pmcentrez&rendertype=abstract},
	doi = {10.1371/journal.pone.0085092},
	abstract = {A lack of behavioural engagement in health promotion or disease prevention is a problem across many health domains. In these cases where people face a genuine danger, a reduced focus on threat and low levels of anxiety or worry are maladaptive in terms of promoting protection or prevention behaviour. Therefore, it is possible that increasing the processing of threat will increase worry and thereby enhance engagement in adaptive behaviour. Laboratory studies have shown that cognitive bias modification (CBM) can increase or decrease anxiety and worry when increased versus decreased processing of threat is encouraged. In the current study, CBM for interpretation (CBM-I) is used to target engagement in sun protection behaviour. The goal was to investigate whether inducing a negative rather than a positive interpretation bias for physical threat information can enhance worry elicited when viewing a health campaign video (warning against melanoma skin cancer), and consequently lead to more adaptive behaviour (sun protection). Participants were successfully trained to either adopt a positive or negative interpretation bias using physical threat scenarios. However, contrary to expectations results showed that participants in the positive training condition reported higher levels of worry elicited by the melanoma video than participants in the negative training condition. Video elicited worry was, however, positively correlated with a measure of engagement in sun protection behaviour, suggesting that higher levels of worry do promote adaptive behaviour. These findings imply that more research is needed to determine under which conditions increased versus decreased processing of threat can drive adaptive worry. Various potential explanations for the current findings and suggestions for future research are discussed.},
	number = {1},
	journal = {PloS one},
	author = {Notebaert, Lies and Chrystal, Jessica and Clarke, Patrick J F and Holmes, Emily a and MacLeod, Colin},
	year = {2014},
	pmid = {24416344},
	keywords = {Female, Humans, Male, Young Adult, Adolescent, Cognition, Anxiety, Anxiety: psychology, Health Behavior, Adaptation, Physiological, Melanoma, Melanoma: prevention \& control, Melanoma: psychology, Patient Acceptance of Health Care, Patient Acceptance of Health Care: psychology, Skin Neoplasms, Skin Neoplasms: prevention \& control, Skin Neoplasms: psychology, Sunscreening Agents, Sunscreening Agents: therapeutic use},
	pages = {e85092},
	annote = {Note that the potential benefits of encouraging increasing processing of threatening information in driving adaptive behaviour. This is the first study to examine the potential benefits of encouraging enhanced processing of threatening information via CBM.


This raises interesting thoughts regarding the context-specificity of processing biases - a la certain biases are beneficial in certain circumstances.


The study induced a positive interpretation bias in half of the participants, and a negative interpretation bias in the other half (and demonstrated that they sig. differed in bias).

Contrary to predictions, the positive training group reported higher levels of worry in response to the melanoma video than the negative training. Larger increases in worry were associated with more engagement in protection in the lost luggage game.


The authors suggest that it is possible that the type of worry might be important. Prior research showing lower worry following positive CBM-I measured things like thought intrusions - whereas this study measured highly relevant worry given the context, as opposed to the maladaptive task-irrelevant rumination for example.},
	file = {Notebaert et al., 2014 - when we should worry more.pdf:C\:\\Users\\Sam\\Documents\\DPhil\\Notebaert et al., 2014 - when we should worry more.pdf:application/pdf}
}

