@article{Harmer2013,
	title = {'{It}'s the way that you look at it'--a cognitive neuropsychological account of {SSRI} action in depression.},
	volume = {368},
	issn = {1471-2970},
	url = {http://rstb.royalsocietypublishing.org/content/368/1615/20120407.long},
	doi = {10.1098/rstb.2012.0407},
	abstract = {The fact that selective serotonin reuptake inhibitors (SSRIs) have antidepressant effects in some patients supports the notion that serotonin plays a role in the mode of action of antidepressant drugs. However, neither the way in which serotonin may alleviate depressed mood nor the reason why several weeks needs to elapse before the full antidepressant effect of treatment is expressed is known. Here, we propose a neuropsychological theory of SSRI antidepressant action based on the ability of SSRIs to produce positive biases in the processing of emotional information. Both behavioural and neuroimaging studies show that SSRI administration produces positive biases in attention, appraisal and memory from the earliest stages of treatment, well before the time that clinical improvement in mood becomes apparent. We suggest that the delay in the clinical effect of SSRIs can be explained by the time needed for this positive bias in implicit emotional processing to become apparent at a subjective, conscious level. This process is likely to involve the re-learning of emotional associations in a new, more positive emotional environment. This suggests intriguing links between the effect of SSRIs to promote synaptic plasticity and neurogenesis, and their ability to remediate negative emotional biases in depressed patients.},
	journal = {Philosophical transactions of the Royal Society of London. Series B, Biological sciences},
	author = {Harmer, Catherine J and Cowen, Philip J},
	year = {2013},
	pmid = {23440467},
	note = {ISBN: 1471-2970},
	keywords = {Citalopram, Citalopram: administration \& dosage, Cognitive Therapy, Humans, Serotonin Uptake Inhibitors, Serotonin Uptake Inhibitors: administration \& dosa, Adaptation, Psychological, Depression, Anxiety, Animals, Time Factors, Fear, Antidepressive Agents, Antidepressive Agents: pharmacology, Anxiety: drug therapy, Citalopram: pharmacology, Depression: drug therapy, Drug Synergism, Fear: drug effects, Fear: psychology, Neuropsychology, Psychological: drug effects, Serotonin Uptake Inhibitors: pharmacology, Synaptic Transmission},
	pages = {20120407},
	annote = {Harmer and Cowen (2013) present a cognitive neuropsychological account of antidepressant action within which antidepressants can produce rapid changes in the processing of emotional information which plays a key role in the "delayed" theraputic effect. The beneficial effects of these changes to cognitive processing require 're-learning' a range of emotional responses/associations which would take time and environment exposure.

It is interesting to note that negative biases in information processing, which have long been a target of cognitive behavioural therapy are also acted on by antidepressant drugs, albeit at an implicit and not explicit level.




also, potential refs are highlighted. check these please},
	file = {Harmer and Cohen 2013 It’s the way that you look at it– a cognitive neuropsychological account of SSRI action in depression..pdf:C\:\\Users\\Sam\\Documents\\DPhil\\Harmer and Cohen 2013 It’s the way that you look at it– a cognitive neuropsychological account of SSRI action in depression..pdf:application/pdf}
}

