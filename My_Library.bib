	year = {2011},
	pmid = {21451959},
	note = {ISBN: 1573-3297 (Electronic){\textbackslash}r0001-8244 (Linking)},
	keywords = {Depression, Anxiety, Internalizing disorders, Longitudinal twin studies, Well-being},
	pages = {641--650},
	file = {Kendler et al. - 2011 - The relationship between the genetic and environmental influences on common internalizing psychiatric disorders.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Kendler et al. - 2011 - The relationship between the genetic and environmental influences on common internalizing psychiatric disorders.pdf:application/pdf}
