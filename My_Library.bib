@article{Genet2013,
	title = {Flexibility is not always adaptive: {Affective} flexibility and inflexibility predict rumination use in everyday life},
	volume = {27},
	issn = {0269-9931},
	doi = {10.1080/02699931.2012.733351},
	abstract = {The present study examined whether cognitive flexibility in the processing of emotional material (i.e., affective flexibility) predicts the use of rumination in response to negative events in daily life. One hundred fifty-seven undergraduate participants completed daily diaries for six consecutive days. Affective flexibility was measured with a novel task-switching paradigm using emotional pictures. Results show that affective inflexibility when switching away from processing the emotional meaning of negative material was associated with increased use of rumination in daily life. In contrast, affective inflexibility when switching away from processing the emotional meaning of positive material was related to decreased use of rumination. Importantly, affective flexibility predicted use of rumination beyond non-affective measures of executive functioning. This is the first study to show that inflexibility is not uniformly associated with increased rumination but that inflexibility in the processing of positive material can predict lower levels of rumination.},
	number = {4},
	journal = {Cognition \& Emotion},
	author = {Genet, Jessica J. and Malooly, Ashley M. and Siemer, Matthias},
	year = {2013},
	pmid = {23088513},
	note = {ISBN: 0269-9931},
	keywords = {emotion, task switching, rumination, cognitive flexibility, negative life events},
	pages = {1--11},
	annote = {Extracted Annotations
this is potentially an important point/paper, as it argues that the same strategy may be used appropriately or innapropriately, but depending on individual's flexibility(note on p. 2)},
	annote = {Rumination has been found to be related to the ability to remove irrelevant negative information from working memory (e.g. joormann and gotlib, 2008). Further, De Lissnyder et al. 2010 found that ruminators showed inhibition impairments specific to the processing of negative emotional information. 

The authirs hypothesised that affective flexibility would predict rumination use in response to daily negative events above and beyond that of non-affective measures of xecutive control. 
In addition they predicted that affetive inflexibility in switching from positive material would be related to decreased use of rumination. 

They used a novel affective-switching task, similar to Garnet \& Siemer's (2011). 
The task used IAPS pictures split into positive and negative, and with 2+ people or less than 2 people. Pictures were presented and participants asked to sort the pictures based on an affective (classify as positive or negative), or non-affective (the number of people). lower switch costs ( switch - repetition RTs) indicate greater affective flexibility. 

The cognitive flexibility task was similar except with the categories of odd-even and higher/lower than 5.
Working memory capacity was assessed with the automated reading span test. 

Daily diary measures were taken for6 days. Participants reported on the most negative event of the day, and rate how unpleasant it was. They then answered six modified rumination items from the Ruminative response scale targeted at that specific event, rather than ruminative tendencies in general. 
Participants also reported daily mood with high and low arousal negative items. Depressive symptoms were also reported.

random coefficient modelling approach was used for nalysis, with daily observations of rumination (level 1) nested within people (level 2).

In the diary data, only unpleasant events rated hihgly were included in the analysis.

In the overall model, only the switch costs that resulted from switching away from focus on the positive and negative pictures predicted levels of daily rumination. 

the data supported the hypotheses. rumination was positively associated with higher switching costs in negative images, and higher costs when switching from positive associated with lower rumination. 

In essence, cognitive inflexibility for positive emotional content was associated with reduced rumination. 
But these analyses were only signitficant for the switch costs when the rule was non-affective. 
[important point for my own stuff, e.g. the IST, is that the key thing there was during the emotional task, but ]


In discussion, the authors note that it was switches away from the emotional material that predicted rumination. Also a really important point that they made is that the same process and task, but under different emotional conditions hihglighted adaptive and maladaptive uses of that process.},
	file = {Genet, Malooly, Siemer - 2012 - Flexibility is not always adaptive Affective flexibility and iGenet, J. J., Malooly, A. M., & Siemer, M..pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Genet, Malooly, Siemer - 2012 - Flexibility is not always adaptive Affective flexibility and iGenet, J. J., Malooly, A. M., & Siemer, M..pdf:application/pdf}
}

@article{Malooly2013,
	title = {Individual differences in reappraisal effectiveness: the role of affective flexibility.},
	volume = {13},
	issn = {1931-1516},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/23163706},
	doi = {10.1037/a0029980},
	abstract = {The present study examined the relation between a specific type of executive control and cognitive emotion regulation. The authors propose that successful reappraisal is related to "affective flexibility": The ability to flexibly attend to and disengage from emotional aspects of a situation or a stimulus. A new affective task-switching paradigm that required participants to shift between categorizing positive and negative affective pictures according to emotional or nonemotional features was used to assess individual differences in affective flexibility. The results showed that greater affective flexibility (less switch costs) predicted the ability to use reappraisal to down-regulate emotions in response to a sad film clip. In particular, more efficient shifts toward the neutral aspects of negative pictures and toward the emotional aspects of positive pictures were found to predict reappraisal ability. The results imply that executive control of emotional material is a capacity that is closely associated with effective reappraisal.},
	number = {2},
	journal = {Emotion (Washington, D.C.)},
	author = {Malooly, Ashley M and Genet, Jessica J and Siemer, Matthias},
	year = {2013},
	pmid = {23163706},
	note = {ISBN: 1931-1516 (Electronic){\textbackslash}n1528-3542 (Linking)},
	keywords = {Female, Humans, Male, Young Adult, Attention, Attention: physiology, Adaptation, Psychological, Adaptation, Psychological: physiology, Individuality, Affect, Reaction Time, Affect: physiology, Executive Function, Executive Function: physiology},
	pages = {302--13},
	file = {Malooly, Genet, Siemer - 2013 - Individual differences in reappraisal effectiveness the role of affective flexibility.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Malooly, Genet, Siemer - 2013 - Individual differences in reappraisal effectiveness the role of affective flexibility.pdf:application/pdf}
