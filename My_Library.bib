	note = {ISBN: 1664-1078},
	keywords = {Anxiety, Threat, Attentional bias, Dot probe, Reliability, ERPs, IAPS, N2pc},
	file = {Kappenman et al. - 2014 - Behavioral and ERP measures of attentional bias to threat in the dot-probe task Poor reliability and lack of c.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Kappenman et al. - 2014 - Behavioral and ERP measures of attentional bias to threat in the dot-probe task Poor reliability and lack of c.pdf:application/pdf}
