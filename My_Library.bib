	journal = {Journal of Clinical Psychiatry},
	author = {Sheehan, David V. and Sheehan, Kathy H. and Shytle, R. Douglas and Janavs, Juris and Bannon, Yvonne and Rogers, Jamison E. and Milo, Karen M. and Stock, Saundra L. and Wilkinson, Berney},
