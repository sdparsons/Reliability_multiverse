	author = {Blanchard, D. Caroline and Griebel, Guy and Pobbe, Roger and Blanchard, Robert J.},
	year = {2011},
	pmid = {21056591},
	note = {Publisher: Elsevier Ltd
ISBN: 1873-7528 (Electronic){\textbackslash}n0149-7634 (Linking)},
	keywords = {Anxiety, Vigilance, Fear, Defensive behaviors, Human defenses, Mirror neurons, Risk assessment},
	pages = {991--998},
	file = {Blanchard et al. - 2011 - Risk assessment as an evolved threat detection and analysis process.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Blanchard et al. - 2011 - Risk assessment as an evolved threat detection and analysis process.pdf:application/pdf}
