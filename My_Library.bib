@article{Genet2011,
	title = {Flexible control in processing affective and non-affective material predicts individual differences in trait resilience.},
	volume = {25},
	issn = {1464-0600},
	doi = {10.1080/02699931.2010.491647},
	abstract = {Trait resilience is a stable personality characteristic that involves the self-reported ability to flexibly adapt to emotional events and situations. The present study examined cognitive processes that may explain individual differences in trait resilience. Participants completed self-report measures of trait resilience, cognitive flexibility and working memory capacity tasks, and a novel affective task-switching paradigm that assesses the ability to flexibly switch between processing the affective versus non-affective qualities of affective stimuli (i.e., flexible affective processing). As hypothesised, cognitive flexibility and flexible affective processing were unique predictors of trait resilience. Working memory capacity was not predictive of trait resilience, indicating that trait resilience is tied to specific cognitive processes rather than overall better cognitive functioning. Cognitive flexibility and flexible affective processing were not associated with other trait measures, suggesting that these flexibility processes are unique to trait resilience. This study was among the first to investigate the cognitive abilities underlying trait resilience.},
	number = {2},
	journal = {Cognition \& emotion},
	author = {Genet, Jessica J and Siemer, Matthias},
	year = {2011},
	pmid = {21432680},
	note = {ISBN: 0269-9931{\textbackslash}r1464-0600},
	keywords = {resilience, executive control, task-switching, cognitive flexibility, death of a loved, events such as the, life, numerous studies have, one or being the, people differ in how, they react to negative, victim of sexual assault},
	pages = {380--388},
	annote = {Extracted Annotations
this is actually very similar to my arguments(note on p. 3)},
	annote = {Psychological flexibility is a core component of resilience, however has not been examined directly as yet. Cognitive flexibility involves 2 components, inhibition and shifting (Miyake et al 2000). Ochsner \& Gross 2007 suggest that cognitive flexibility is related to emotion regulation. This may be particularly true by the process of cognitive flexibility enabling effective emotion regulation thus promoting trait resilience. 

They used the odd/even or lower/higher than 5 task to assess general cognitive flexibility. In the affective task, they sorted words by whether they were adjectives or nouns, or whether they were positive/negative. Switching was investigated in a similar way to the other Ganet and Siemer paper. WM was assessed with an automated reading span test.

They combined the CDRISC and ER89 scales to create a total score (with the z values).

resilience was predicted in a multiple regression by both affective switching costs measures, and general cognitive flexibility, but not by WMC.

This is one of the first demonstrations of a relationship between cognitive flexibility (task switching) and trait resilience in an adult population. 

They argue that resilience is not generally related to general executive function, but rather that trait resilience is tied to specific processes related to flexibility.},
	file = {Genet, Siemer - 2011 - Flexible control in processing affective and non-affective material predicts individual differences in trait resi.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Genet, Siemer - 2011 - Flexible control in processing affective and non-affective material predicts individual differences in trait resi.pdf:application/pdf}
