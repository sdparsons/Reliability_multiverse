@article{mcnally_attention_2013-1,
	title = {Attention bias modification for reducing speech anxiety},
	volume = {51},
	issn = {00057967},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0005796713001769},
	doi = {10.1016/j.brat.2013.10.001},
	abstract = {The mechanisms mediating the anxiolytic effects of attention bias modiﬁcation (ABM) remain unclear. Accordingly, we randomly assigned speech-anxious subjects to receive four sessions of one of three training conditions: ABM, inverse ABM, and control. In the ABM condition, subjects viewed pairs of photographs of models displaying facial expressions of disgust and joy on a computer screen. Probes always replaced the positive face, and subjects pushed a button to indicate the identity of the probe (E or F) as rapidly as possible. In the inverse condition, the probes always replaced the negative face, and in the control condition, the probes replaced each face type equally often. After four training sessions, all groups exhibited statistically indistinguishable, but signiﬁcant, reductions on self-report, behavioral, and physiological measures of speech anxiety. Self-report and behavioral measures of attentional control improved likewise. Contrary to early studies, ABM was not superior to control procedures in producing reductions on measures of social anxiety.},
	language = {en},
	number = {12},
	urldate = {2019-01-16},
	journal = {Behaviour Research and Therapy},
	author = {McNally, Richard J. and Enock, Philip M. and Tsai, Cynthia and Tousian, Mona},
	month = dec,
	year = {2013},
	pages = {882--888}
}

