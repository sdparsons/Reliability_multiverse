	pages = {S10--6},
	file = {Epel, Lithgow - 2014 - Stress biology and aging mechanisms toward understanding the deep connection between adaptation to stress and lon.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Epel, Lithgow - 2014 - Stress biology and aging mechanisms toward understanding the deep connection between adaptation to stress and lon.pdf:application/pdf}
