	pages = {819--833},
	file = {Grol and De Raedt - 2018 - The effect of positive mood on flexible processing.pdf:C\:\\Users\\Sam\\Zotero\\storage\\2G888WVT\\Grol and De Raedt - 2018 - The effect of positive mood on flexible processing.pdf:application/pdf}
}

