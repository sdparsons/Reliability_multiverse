@article{lau_cognitive_2013-2,
	title = {Cognitive bias modification of interpretations: {A} viable treatment for child and adolescent anxiety?},
	volume = {51},
	issn = {00057967},
	shorttitle = {Cognitive bias modification of interpretations},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0005796713001174},
	doi = {10.1016/j.brat.2013.07.001},
	abstract = {Anxiety disorders in children and adolescents are common and impairing. As many patients do not beneﬁt from e or have difﬁculties accessing e frontline treatments, novel, effective and easy-to-deliver interventions are needed. Cognitive Bias Modiﬁcation of Interpretations (CBM-I) training has been used to treat adult anxiety disorders. CBM-I methods train individuals to endorse benign rather than negative resolutions of ambiguous cues. Developmental extensions of CBM-I are important for several reasons. First, implementing CBM-I in symptomatic children and adolescents may facilitate early preventative gains. Second, as training uses simple learning mechanisms, CBM-I may reﬂect a developmentallysuitable strategy for shaping adaptive processing styles. Third, as this age range involves protracted neurocognitive maturation and associated plasticity, administering CBM-I early could drive powerful, long-lasting beneﬁts for emotional development. Finally, data from CBM-I studies could inform the cognitive mechanisms involved in the genesis of early-emerging anxiety. This paper provides the ﬁrst organised review of CBM-I studies conducted in children and adolescents, and contains suggestions for future research that may help realise the therapeutic potential of early CBM-I interventions.},
	language = {en},
	number = {10},
	urldate = {2019-01-16},
	journal = {Behaviour Research and Therapy},
	author = {Lau, Jennifer Y.F.},
	month = oct,
	year = {2013},
	pages = {614--622}
}

@article{lebel_sexy_2011-1,
	title = {Sexy {But} {Often} {Unreliable}: {The} {Impact} of {Unreliability} on the {Replicability} of {Experimental} {Findings} {With} {Implicit} {Measures}},
	volume = {37},
	issn = {0146-1672, 1552-7433},
	shorttitle = {Sexy {But} {Often} {Unreliable}},
	url = {http://journals.sagepub.com/doi/10.1177/0146167211400619},
	doi = {10.1177/0146167211400619},
	abstract = {Implicit measures have contributed to important insights in almost every area of psychology. However, various issues and challenges remain concerning their use, one of which is their considerable variation in reliability, with many implicit measures having questionable reliability. The goal of the present investigation was to examine an overlooked consequence of this liability with respect to replication, when such implicit measures are used as dependent variables in experimental studies. Using a Monte Carlo simulation, the authors demonstrate that a higher level of unreliability in such dependent variables is associated with substantially lower levels of replicability. The results imply that this overlooked consequence can have farreaching repercussions for the development of a cumulative science. The authors recommend the routine assessment and reporting of the reliability of implicit measures and also urge the improvement of implicit measures with low reliability.},
	language = {en},
	number = {4},
	urldate = {2019-01-16},
	journal = {Personality and Social Psychology Bulletin},
	author = {LeBel, Etienne P. and Paunonen, Sampo V.},
