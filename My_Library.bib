	keywords = {Female, Humans, Male, Young Adult, Adult, Emotions, Regression Analysis, Depression, Adolescent, Questionnaires, Resilience, Psychological, Genetic Predisposition to Disease, Serotonin Plasma Membrane Transport Proteins, Serotonin Plasma Membrane Transport Proteins: gene, Depression: genetics, Genetic Variation, Multivariate Analysis, Neurotic Disorders, Neurotic Disorders: complications},
	pages = {900--6},
